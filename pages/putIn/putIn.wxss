page {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.container {
  background-color: #fff;
  overflow-y: auto;
  padding: 40px 20px 30px;
}

.item {
  align-items: center;
  /* border-bottom: 1px solid #999; */
  display: flex;
  justify-content: space-between;
  margin-bottom: 10px;
  width: 100%;
}



.label {
  font-weight: 700;
  text-align: left;
}

.my-button {
  background-color: #33acdb;
  border: none;
  border-radius: 5px;
  color: #fff;
  cursor: pointer;
  font-size: 16px;
  height: 40px;
}


.navigator {
  display: flex;
  margin-top: 10px;
  width: 100%;
}

.box,.navigator {
  align-items: center;
  justify-content: space-between;
}

.box {
  background-color: #fff;
  border: 1px solid #000;
  border-radius: 10px;
  box-shadow: 2px 2px 5px 3px rgba(0,0,0,.2);
  padding: 20px;
}
.row {
  align-items: center;
  margin-top: 10px;
}

.lable2 {
  color: #333;
  font-weight: 700;
  width: 60%;
}

.lable3 {
  color: #333;
  font-weight: 700;
  border-bottom: 1px solid #999;
  width: 60%;
}

.product-description,.serial-number {
  color: #000;
  flex-grow: 1;
  font-size: 14px;
}