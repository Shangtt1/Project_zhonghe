.page {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.item {
  align-items: center;
  border-bottom: 1px solid #999;
  display: flex;
  justify-content: space-between;
  margin: 20px;
  padding-bottom: 10px;
  width: 90%;
}

.my-button {
  background-color: #4caf50;
  border: none;
  border-radius: 5px;
  color: #fff;
  cursor: pointer;
  font-size: 16px;
}

.inputLongContent {
  font-size: 14px;
  width: 60%;
}
